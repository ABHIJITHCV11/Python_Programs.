   "source": [
    "def ls(arr,ind,t):\n",
    "    l = []\n",
    "    if ind == len(arr):\n",
    "        return l\n",
    "    if arr[ind] == t:\n",
    "        l.append(ind)\n",
    "    fl = l + ls(arr,ind+1,t)\n",
    "    return fl"
   ]
